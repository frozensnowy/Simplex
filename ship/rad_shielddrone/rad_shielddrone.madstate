RAD_SHIELDDRONE_RepairStart_OnSet = function(ship) 
    setState(ship, "RepairStart", 1)     
end
RAD_SHIELDDRONE_RepairDo_OnSet = function(ship)        
    startEffect(ship, "Repairing")    
    setState(ship, "RepairDo", 1)
end
RAD_SHIELDDRONE_RepairEnd_OnSet = function(ship)    
    endEffect(ship, "Repairing")    
    setState(ship, "RepairEnd", 1)    
end

