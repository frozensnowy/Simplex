function kad_hybridspider_Open_OnSet(ship)
    startAnim(ship, "Open")
    setPauseTime(ship, "Open", 1000)
	startEffect(ship, "Open")
end

function kad_hybridspider_Docked_OnSet(ship)
    startAnim(ship, "Latch")
    setPauseTime(ship, "Latch", 1000)
end
