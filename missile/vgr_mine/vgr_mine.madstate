function Vgr_Mine_Open_OnSet(ship)
    startAnim(ship, "Open")
    setPauseTime(ship, "Open", 1000)
end

function Vgr_Mine_Close_OnSet(ship)
    stopAnim(ship, "Open")
    startAnim(ship, "Close")
end
